  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Image Tagger, Debug Mode and Save Cropped Images",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/taglist/main.go",
      "args": ["-images_path", "${workspaceFolder}/images/file_list.txt", "-tags_path", "${workspaceFolder}/tags.json", "-out", "${workspaceFolder}/out", "-mode", "fit", "-debug", "-save"],
      "env": {
        "OLLAMA_HOST": "http://localhost:11434"
      }
    }
  ]
}
